{
  "name": "@ladjs/passport",
  "description": "Passport for Lad",
  "version": "4.0.1",
  "author": "Nick Baugh <niftylettuce@gmail.com> (http://niftylettuce.com/)",
  "ava": {
    "failFast": true,
    "verbose": true,
    "serial": true
  },
  "bugs": {
    "url": "https://github.com/ladjs/passport/issues",
    "email": "niftylettuce@gmail.com"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "contributors": [
    "Nick Baugh <niftylettuce@gmail.com> (http://niftylettuce.com/)",
    "Shaun Warman <shaunwarman1@gmail.com> (https://shaunwarman.com/)",
    "shadowgate15 (https://github.com/shadowgate15)"
  ],
  "dependencies": {
    "@ladjs/passport-otp-strategy": "^2.0.0",
    "boolean": "^3.2.0",
    "koa-passport": "^4.1.4",
    "lodash": "^4.17.21",
    "passport-github2": "^0.1.12",
    "passport-google-oauth": "^2.0.0",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.1",
    "@commitlint/config-conventional": "^17.0.0",
    "ava": "^4.2.0",
    "codecov": "^3.8.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.16.0",
    "eslint-config-xo-lass": "^1.0.6",
    "fixpack": "^4.0.0",
    "husky": "^8.0.1",
    "lint-staged": "^12.4.3",
    "nyc": "^15.1.0",
    "passport-local": "^1.0.0",
    "remark-cli": "^10.0.1",
    "remark-preset-github": "^4.0.1",
    "xo": "^0.49.0"
  },
  "engines": {
    "node": ">=12"
  },
  "files": [
    "index.js"
  ],
  "homepage": "https://github.com/ladjs/passport",
  "keywords": [
    "api",
    "auth",
    "authentication",
    "basic",
    "boilerplate",
    "framework",
    "github",
    "google",
    "koa",
    "lad",
    "lass",
    "list",
    "login",
    "mongo",
    "mongodb",
    "mongoose",
    "oauth",
    "oauth2",
    "passport",
    "strategies",
    "strategy",
    "twitter"
  ],
  "license": "MIT",
  "main": "index.js",
  "nyc": {
    "check-coverage": true,
    "lines": 100,
    "functions": 100,
    "branches": 100,
    "reporter": [
      "lcov",
      "html",
      "text"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "remarkConfig": {
    "plugins": [
      "preset-github"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ladjs/passport"
  },
  "scripts": {
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "lint": "xo && remark . -qfo",
    "prepare": "husky install",
    "test": "npm run lint && npm run test-coverage",
    "test-coverage": "cross-env NODE_ENV=test nyc ava"
  },
  "xo": {
    "prettier": true,
    "space": true,
    "extends": [
      "xo-lass"
    ]
  }
}
